    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:vm="sap.ui.comp.variants">
    <Shell id="shell">
        <App id="app">
            <pages>
                <Page  showHeader="false">
                    <!-- title="{i18n>title}" -->
                    <content>
                        <f:DynamicPage id="dynamicPageId" headerExpanded="true">
