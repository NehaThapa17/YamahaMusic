                                                        <Input showValueHelp="true" ></Input>
                                                    </fb:control>
                                                </fb:FilterGroupItem>
                                                <fb:FilterGroupItem groupName="__$INTERNAL$" name="D" label="Document Date" partOfCurrentVariant="true" visibleInFilterBar="true">
                                                    <fb:control>
                                                        <DatePicker change="onChange"/>
                                                    </fb:control>
                                                </fb:FilterGroupItem>
                                                <fb:FilterGroupItem groupName="__$INTERNAL$" name="A1" label="Customer Reference" partOfCurrentVariant="true" visibleInFilterBar="true">
                                                    <fb:control>
                                                        <Input type="Text" change="onChange"/>
                                                    </fb:control>
                                                </fb:FilterGroupItem>
                                                <fb:FilterGroupItem groupName="__$INTERNAL$" name="B" label="Created On" partOfCurrentVariant="true" visibleInFilterBar="true">
                                                    <fb:control>
                                                        <DatePicker change="onChange"/>
                                                    </fb:control>
                                                </fb:FilterGroupItem>


                                                <fb:FilterGroupItem groupName="__$INTERNAL$" name="C" label="Created By" partOfCurrentVariant="true" visibleInFilterBar="true">
                                                    <fb:control>
                                                        <!-- <ComboBox change="onChange">
                                                            <items>
                                                                <core:Item key="001" text="EMEA"/>
                                                                <core:Item key="002" text="America"/>
                                                                <core:Item key="003" text="APJ"/>
                                                            </items>
                                                        </ComboBox> -->
                                                        <Input showValueHelp="true" ></Input>
                                                    </fb:control>
                                                </fb:FilterGroupItem>
                                                
                                                <fb:FilterGroupItem groupName="__$INTERNAL$" name="os" label="Order Status" partOfCurrentVariant="true" visibleInFilterBar="true">
                                                    <fb:control>
                                                        <Select ></Select>
                                                    </fb:control>
                                                </fb:FilterGroupItem>
                                                <fb:FilterGroupItem groupName="__$INTERNAL$" name="ds" label="Delivery Status" partOfCurrentVariant="true" visibleInFilterBar="true">
                                                    <fb:control>
                                                        <Select ></Select>
                                                    </fb:control>
                                                </fb:FilterGroupItem>
                                                <fb:FilterGroupItem groupName="__$INTERNAL$" name="is" label="Invoice Status" partOfCurrentVariant="true" visibleInFilterBar="true">
                                                    <fb:control>
                                                        <Select ></Select>
                                                    </fb:control>
                                                </fb:FilterGroupItem>
                                                
                                            </fb:filterGroupItems>
                                        </fb:FilterBar>
                                    </f:content>
                                </f:DynamicPageHeader>
                            </f:header>

                            <!-- DynamicPage Content -->
                            <f:content>
                                <Table id="idProductsTable" inset="false">
                                <headerToolbar>
                                <OverflowToolbar>
                                    <Title text="Sales Orders (3)"/>
                                    <ToolbarSpacer/>
                                    <Button text="Create Sales Order" type="Emphasized" press="onPressCreateSO"/>
                                    <!-- <Button icon="sap-icon://action-settings"/> -->
                                </OverflowToolbar>
                                </headerToolbar>

                                    <columns>
                                        <Column minScreenWidth="Tablet" demandPopin="true">
                                            <Text text="Sales Document"/>
