<core:FragmentDefinition xmlns="sap.ui.comp.valuehelpdialog"
    xmlns:m="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core">

    <ValueHelpDialog title="Document No" ok=".onValueHelpOkPress" cancel=".onValueHelpCancelPress" afterClose=".onValueHelpAfterClose" key="docNo" descriptionKey="dsc" supportMultiselect="false">
        <filterBar>
            <fb:FilterBar advancedMode="true" search=".commonFilterBarSearch">
                <fb:filterGroupItems>
                    <fb:FilterGroupItem groupName="__$INTERNAL$" name="docNo" label="DocumentNo" visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="docNo" />
                        </fb:control>
                    </fb:FilterGroupItem>
                </fb:filterGroupItems>
                <fb:filterGroupItems>
                    <fb:FilterGroupItem groupName="__$INTERNAL$" name="dsc" label="Description" visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="dsc" />
                        </fb:control>
                    </fb:FilterGroupItem>
                </fb:filterGroupItems>
            </fb:FilterBar>
        </filterBar>
    </ValueHelpDialog>
</core:FragmentDefinition>